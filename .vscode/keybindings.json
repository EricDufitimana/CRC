{
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [";"],
      "after": [":"]
    },
    {
      "before": ["<CapsLock>"],
      "after": ["<Esc>"]
    },
    {
      "before": ["<S-CapsLock>"],
      "after": ["<Esc>"]
    },
    {
      "before": ["<S-Space>"],


      "after": []
    },
    {
      
      "before": [" "],
      "commands": ["editorScroll", { "to": "down", "by": "halfPage", "revealCursor": true }]
    },
    {
      "before": ["."],
      
      "after": ["8", "l"]
    },
    {
      "before": [">"],
      "after": ["8", "h"]
    },
    {
      "before": ["m"],
      "after": ["j", "j", "j", "j", "j", "j", "j", "j", "j", "j"]
    },
    {
      "before": ["n"],
      "after": ["k", "k", "k", "k", "k", "k", "k", "k", "k", "k"]
    }

  ]
}